using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;


public class Person
{
    public double income;
    public double age;
    public Person(double income, double age)
    {
        this.income = income;
        this.age =age;
    }
    public void SetAge(double age)
    {
        this.age = age;
    }

    public void SetIncome(double income)
    {
        this.income = income;
    }



}

